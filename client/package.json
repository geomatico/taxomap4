{
  "name": "taxomap4",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/main.js",
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "test": "npm run lint && karma start karma.config.js",
    "update-data": "cd static/data && rm -rf * && ../../../devops/extract_data_from_db.sh && cd -",
    "start": "webpack serve --env dev",
    "build": "npm test && webpack --env prod",
    "stats": "webpack --env prod --json > stats.json",
    "storybook": "start-storybook -s ./static -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "npm run lint:js && npm run lint:i18n",
    "lint:js": "eslint src",
    "lint:i18n": "eslint --fix --format node_modules/eslint-plugin-i18n-json/formatter.js src/i18n/*.json",
    "prepare": "cd .. && husky install .husky",
    "docker-prepare": "docker run --rm -t -e TAXOMAP_BACKEND_HOST=localhost:8000 -v $PWD/../:/repo mullnerz/ansible-playbook ansible-playbook -i /repo/devops/inventories/dev.yml /repo/devops/render-templates.yml -e '{use_frontend: True, use_proxy: True}'",
    "docker-start": "npm run docker-prepare && docker compose -f ../devops/docker-compose.yml up",
    "docker-clean": "npm run docker-prepare && docker compose -f ../devops/docker-compose.yml down --rmi local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geomatico/taxomap4.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/geomatico/taxomap4/issues"
  },
  "homepage": "https://labs.geomatico.es/taxomap4",
